<!DOCTYPE html><html lang="en">

    <%- include('./partials/head.ejs') %> 

<body>

    <%- include('./partials/head.ejs') %> 

	<main class="main">
		
		<%- include('./partials/header.ejs') %> 

        

			<!-- Begin services -->
<section class="section services">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="section-heading heading-center">
                    <div class="section-subheading">Seleccione una Opción</div>
                    <h2>Reenvío de Facturas</h2>
                </div> 
            </div>
            <div class="col-lg-6 col-md-3 col-12 item">
                <a href="./resendPage" class="iitem item-style">
                    <div class="iitem-icon">
                        <i class="material-icons material-icons-outlined md-48">receipt_long</i>
                    </div>
                    <div class="iitem-icon-bg">
                        <i class="material-icons material-icons-outlined">receipt_long</i>
                    </div>
                    <h3 class="iitem-heading item-heading-large">Facturas en Proceso</h3>
                    <div class="iitem-desc">Reenvío de Facturas en estado PROCESANDO</div>
                </a>    
            </div>
            <div class="col-lg-6 col-md-3 col-12 item">
                <a href="single-services.html" class="iitem item-style">
                    <div class="iitem-icon">
                        <i class="material-icons material-icons-outlined md-48">receipt</i>
                    </div>
                    <div class="iitem-icon-bg">
                        <i class="material-icons material-icons-outlined">receipt</i>
                    </div>
                    <h3 class="iitem-heading item-heading-large">Facturas en Error</h3>
                    <div class="iitem-desc">Próximamente</div>
                </a>
            </div>          
            <!-- <div class="col-12">
                <div class="section-btns justify-content-center">
                    <a href="services.html" class="btn btn-with-icon btn-w240 ripple">
                        <span>See All Services</span>
                        <svg class="btn-icon-right" viewBox="0 0 13 9" width="13" height="9"><use xlink:href="/img/sprite.svg#arrow-right"></use></svg>
                    </a>
                </div> -->
            </div>
        </div>
    </div>
</section><!-- End services -->



			<!-- Begin counter section -->
            

<section class="section">
    <div class="container">
        <div class="row spincrement-container">
            <div class="col-xl-5 offset-xl-2 col-lg-6 offset-lg-1 col-12">
                <div class="main-counter">
                    <div class="main-counter-item">
                        <div class="main-counter-item-center">
                            <div> 
                                <div class="main-counter-numb spincrement" data-from="0" data-to=<%=uniqueSucur.length%> >0</div>
                                <div class="main-counter-heading">Sedes <br> con incovenientes </div>
                            </div>
                        </div>
                        <div class="main-counter-item-circ"></div>
                    </div>
                </div>
            </div> 
            <div class="col-xl-4 offset-xl-1 col-lg-3 offset-lg-1 col-12 counter-items items">
                <div class="counter-item item">
                    <div class="counter-item-numb"><span class="spincrement" data-from="0" data-to=<%=documents.length%> >0</span></div>
                    <p class="counter-item-heading">Facturas Procesando</p>
                </div>
                <div class="counter-item item"> 
                    <div class="counter-item-numb spincrement" data-from="0" data-to="0">0</div>
                    <p class="counter-item-heading">Facturas en Error (en construcción)</p>
                </div>
                <div class="counter-item item">
                    <div class="counter-item-numb spincrement" data-from="0" data-to="0">0</div>
                    <p class="counter-item-heading">Facturas Pendientes (en construcción)</p>
                </div>
            </div>
        </div>
    </div>


</section><!-- End counter section -->




		</div>
       

		<%- include('./partials/footer.ejs') %> 



	</main><!-- End main -->

    <%- include('./partials/scripts.ejs') %> 

	
